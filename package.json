{
  "name": "custom-wordpress-sage-theme",
  "private": true,
  "scripts": {
    "predev": "curl -i https://dev-phassociation.pantheonsite.io",
    "build": "set NODE_ENV=production  &&  webpack --config ./webpack/build.js",
    "build:hashed": "set NODE_ENV=production  &&  webpack --env hashFilenames --config ./webpack/build.js",
    "dev": "set NODE_ENV=development  &&  webpack --progress --config ./webpack/dev.js",
    "dev:hashed": "set NODE_ENV=development  &&  webpack --progress --env hashFilenames --config ./webpack/dev.js",
    "translate": "npm run translate:pot && npm run translate:update",
    "translate:pot": "wp i18n make-pot . ./resources/lang/sage.pot --include=\"theme.json,patterns,app,resources\"",
    "translate:update": "for file in ./resources/lang/*.po; do wp i18n update-po ./resources/lang/sage.pot $file; done",
    "translate:compile": "npm run translate:mo && npm run translate:js",
    "translate:js": "wp i18n make-json ./resources/lang --pretty-print",
    "translate:mo": "wp i18n make-mo ./resources/lang ./resources/lang",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:js": "eslint resources/scripts",
    "lint:js-fix": "eslint --fix",
    "lint:css": "stylelint \"resources/**/*.{css,scss,vue}\"",
    "test": "npm run lint"
  },
  "devDependencies": {
    "@babel/core": "^7.26.8",
    "@babel/preset-env": "^7.26.8",
    "@babel/preset-react": "^7.26.3",
    "@fullhuman/postcss-purgecss": "^7.0.2",
    "@popperjs/core": "^2.11.8",
    "@types/bootstrap": "^5.2.10",
    "autoprefixer": "^10.4.20",
    "babel-loader": "^9.2.1",
    "bootstrap": "^5.3.3",
    "clsx": "^2.1.1",
    "compression-webpack-plugin": "^11.1.0",
    "copy-webpack-plugin": "^12.0.2",
    "css-loader": "^7.1.2",
    "css-minimizer-webpack-plugin": "^7.0.0",
    "eslint": "^9.20.1",
    "eslint-config-prettier": "^10.0.1",
    "eslint-webpack-plugin": "^4.2.0",
    "focus-trap": "^7.6.4",
    "globals": "^15.15.0",
    "json-loader": "^0.5.7",
    "mini-css-extract-plugin": "^2.9.2",
    "postcss": "^8.5.2",
    "postcss-loader": "^8.1.1",
    "prettier": "^3.5.1",
    "purgecss": "^7.0.2",
    "query-string": "^9.1.1",
    "reframe.js": "^4.0.2",
    "sass": "1.77.6",
    "sass-loader": "^16.0.4",
    "style-loader": "^4.0.0",
    "stylelint": "^16.14.1",
    "stylelint-config-standard": "^37.0.0",
    "stylelint-config-standard-scss": "^14.0.0",
    "stylelint-webpack-plugin": "^5.0.1",
    "terser-webpack-plugin": "^5.3.11",
    "ts-loader": "^9.5.2",
    "typescript": "^5.7.3",
    "webpack": "^5.98.0",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.0",
    "webpack-manifest-plugin": "^5.0.0",
    "webpack-merge": "^6.0.1"
  },
  "browserslist": [
    "last 2 versions",
    "> 2%"
  ],
  "engines": {
    "node": ">=20.18.0"
  },
  "volta": {
    "node": "20.18.0"
  }
}
